#include <iostream>
#include <locale>
#include "cmath"

using namespace std;

int po4ti_string(const char* stroka1) {
	int i = 0;
	while (*stroka1++)
		++i;
	return i;
}

char* Registr(char* stroka2)
{
	int i = 0, j = 0;
	for (i = 0; i < po4ti_string(stroka2); i++) {
		if (stroka2[i] >= 'A' && stroka2[i] <= 'Z') {
			stroka2[i] += 32;
		}
	}

	return stroka2;

}
